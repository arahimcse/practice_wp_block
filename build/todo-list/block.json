{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "create-block/todo-list",
  "version": "0.1.0",
  "title": "Todo List",
  "category": "widgets",
  "description": "Example block scaffolded with Create Block tool.",
  "example": {},
  "attributes": {
    "content": {
      "type": "string",
      "source": "html",
      "selector": "p"
    },
    "checkboxField": {
      "type": "boolean",
      "default": true
    },
    "radioField": {
      "type": "boolean",
      "default": "yes"
    },
    "textField": {
      "type": "string"
    },
    "toggleField": {
      "type": "boolean"
    },
    "selectField": {
      "type": "string"
    }
  },
  "supports": {
    "html": true,
    "typography": {
      "fitText": true,
      "fontSize": true,
      "lineHeight": true,
      "textAlign": true
    },
    "color": {
      "background": true,
      "text": true,
      "link": true
    },
    "layout": true,
    "dimensions": {
      "aspectRatio": true,
      "minHeight": true
    },
    "shadow": true,
    "position": {
      "sticky": true
    },
    "spacing": {
      "margin": true,
      "padding": true
    },
    "align": true
  },
  "textdomain": "todo-list",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "viewScript": "file:./view.js"
}